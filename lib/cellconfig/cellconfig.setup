; -*- Hen -*-

(define (dynld-name fn)		
  (make-pathname #f fn ##sys#load-dynamic-extension))	

(compile -O3 -d0 -S -s cellconfig.scm -j cellconfig)
(compile -O2 -s cellconfig.import.scm) 

(install-extension

  ; Name of your extension:
  'cellconfig

  ; Files to install for your extension:
  `(,(dynld-name "cellconfig") ,(dynld-name "cellconfig.import")) 

  ; Assoc list with properties for your extension:
  `((version 1.0)))
